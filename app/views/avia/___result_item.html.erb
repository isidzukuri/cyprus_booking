<li class="fly_el">
	<table>
		<% fly["direct_flights"].each_with_index do |seg,i|%>
            <% next if i >0%>
                <tr class="item_elms">
                    <td style="width: 150px;">
                        <%=image_tag "airline_logos/#{seg['airline']}.png" ,:size=>"120x50"%>
                    </td>
                    <td style="width: 250px;">
                        <h3>
                            <%= seg['aircraft']%>
                            <span class="analog"> <%="#{seg['airline']} #{seg['number']}"%></span>
                        </h3>
                        <p class="car_cls"><%=t("all.avia.in_flight",:hour=>seg['duration'].to_i/60,:min=>seg['duration'].to_i%60)%>
                        </p>
                        <b style="font-size: 15px;"><%="#{seg["origin"]} → #{seg["destination"]}"%></b>
                    </td>
                    <td  class="hghg">
                        <p class="fgfg"><%=t("all.avia.dep")%></p>
                                                    <b><%=l(Time.at(seg["departure"].to_i),:format=>:short)%></b>
                                                    <%if true%>
                                                    <p class="fgfg"><%=t("all.avia.arr")%></p>
                                                    <b><%=l(Time.at(seg["arrival"].to_i),:format=>:short)%></b>
                                                    <%end%>
                    </td>
                </tr>
		<%end%>
		<% if fly["return_flights"].present? %>
			<% fly["return_flights"].each_with_index do |seg,i|%>
            <% next if i >0%>
                <tr class="item_elms">
                    <td style="width: 150px;">
                        <%=image_tag "airline_logos/#{seg['airline']}.png" ,:size=>"120x50"%>
                    </td>
                    <td style="width: 250px;">
                        <h3>
                            <%= seg['aircraft']%>
                            <span class="analog"> <%="#{seg['airline']} #{seg['number']}"%></span>
                        </h3>
                        <p class="car_cls"><%=t("all.avia.in_flight",:hour=>seg['duration'].to_i/60,:min=>seg['duration'].to_i%60)%>
                        </p>
                        <b style="font-size: 15px;"><%="#{seg["origin"]} → #{seg["destination"]}"%></b>
                    </td>
                    <td class="hghg">
                        <p class="fgfg"><%=t("all.avia.dep")%></p>
                                                    <b><%=l(Time.at(seg["departure"].to_i),:format=>:short)%></b>
                                                    <%if true%>
                                                    <p class="fgfg"><%=t("all.avia.arr")%></p>
                                                    <b><%=l(Time.at(seg["arrival"].to_i),:format=>:short)%></b>
                                                    <%end%>
                    </td>
                </tr>
			<%end%>
		<%end%>
	</table>
    <table>
        <tr class="item_elms">
            <td class="price pay_btn">
                <span><%="#{(Exchange.convert(:RUR, $currency) *fly['native_prices'].values.first.to_f).to_i} #{t("all.currencies_symbols.#{$currency}")}"%>
                </span>
                <a target="_blank" href="<%= @url.call(@results["search_id"],fly['order_urls'].values.first) %>">
                    <%=t("all.choose")%>
                    <span style="color:#ffffff;font-size: 16pt;">></span>
                </a>
            </td>
        </tr>
<!--         <tr class="item_elms">
            <td  class="pay_btn">
                <a target="_blank" href="<%= @url.call(@results["search_id"],fly['order_urls'].values.first) %>"><%=t("all.choose")%><span>></span></a>
            </td>
        </tr> -->
    </table>
</li>
<script type="text/javascript">
    $(document).ready(function(){
        $(".fly_el").each(function(){
            $(this).find("table:last").height($(this).find("table:first").height())
        })
    })
</script>