<%=book_steps(t("all.cars.book_steps"),0)%>
        <div id="main_content" data-auto-controller="HotelResult" data-key="<%=@results[:key]%>" data-location="<%=@results[:location]%>" data-id="<%=@session_id%>">
            <div id="main" >
                <div id="main_wrap">
                    <div class="header">
                        <h2><%=t("all.search_result")%>: <span><%=@search.to_title%></span> </h2>
                        <div class="sorting">
                            
                            <ul>
                                <li style="margin-right: 10px;"><%=t("all.sort_by")%> </li>
                                <li>
                                    <%=select_tag :sort,options_for_select(t("all.hotels.sort_options").each_pair.map{|b,a| [a,b]})%>
                                </li>

                            </ul>
                        </div>
                    </div>
                    <ul class="result_list">
                        <%@results[:result].each do |hotel|%>
                            <%= render partial:"hotels/result_item",:locals=>{:hotel=>hotel}%>
                        <%end%>
                    </ul>
                        <div class="h_loader">
                            <%=image_tag "spinner.gif"%>
                        </div>
                    <div class="clear"></div>
                </div>
                    <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>